{
  "name": "digcms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:lib": "ng build dig",
    "build:schematics": "tsc -p projects/dig/tsconfig.schematics.json",
    "postbuild:schematics": "copyfiles projects/dig/schematics/collection.json dist/dig/schematics/ -f",
    "build:all": "npm run build:lib && npm run build:schematics",
    "pack:lib": "cd dist/dig && npm pack",
    "lib:all": "npm run build:all && npm run pack:lib",
    "publish": "npm run lib:all && cd dist/dig && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.1.1",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^15.1.1",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@ngrx/effects": "^15.1.0",
    "@ngrx/store": "^15.1.0",
    "@ngrx/store-devtools": "^15.1.0",
    "camelcase": "^7.0.1",
    "dashify": "^2.0.0",
    "firebase": "^9.17.1",
    "ngx-markdown": "^15.1.1",
    "rxfire": "^6.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.4",
    "@angular/cli": "~15.1.4",
    "@angular/compiler-cli": "^15.1.0",
    "@dig-platform/dig": "^15.0.0",
    "@types/dashify": "^1.0.1",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.0",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.1.0",
    "typescript": "~4.9.4"
  }
}
